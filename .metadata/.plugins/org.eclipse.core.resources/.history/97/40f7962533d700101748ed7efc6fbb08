package Animal;

public class Monkey extends Animal {
    public String name = "Monkey";
    
    public Monkey(int age) {
        super(age);
    }
    
    public Monkey() {
        this(150);
    }
    
    public void setDetails() {
        setType("Mammal");
        System.out.println(super.nameM + " " + getType());  
    }
            
}
